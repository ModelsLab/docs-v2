          "model_id": {
            "type": "string",
            "description": "ID of the voice cloning model [Find Models Here](https://modelslab.com/models?feature=audiogen)"
          },
          "pitch": {
            "type": "string",
            "enum": ["m2f", "f2m", "none"],
            "description": "Controls pitch transformation between voices"
          },
          "algorithm": {
            "type": "string",
            "enum": ["rmvpe", "mangio-crepe"],
            "default": "rmvpe",
            "description": "Algorithm used for voice cloning"
          },
          "rate": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
