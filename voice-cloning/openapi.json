      "VoiceCoverRequest": {
        "type": "object",
        "required": ["key", "init_audio", "model_id"],
        "properties": {
          "key": {
            "type": "string",
            "description": "API key to authorize the request"
          },
          "init_audio": {
            "type": "string",
            "description": "URL (YouTube supported) or base64 WAV file for audio to be cloned"
