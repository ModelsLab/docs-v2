{
  "openapi": "3.1.0",
  "info": {
    "title": "ModelsLab Voice API",
    "description": "A comprehensive API for AI-driven voice and audio generation including text-to-speech, voice cloning, music generation, and audio processing capabilities",
    "license": {
      "name": "MIT"
    },
    "version": "6.0.0"
  },
  "servers": [
    {
      "url": "https://modelslab.com/api/v6"
    }
  ],
  "paths": {
    "/voice/fetch/{id}": {
      "post": {
        "summary": "Fetch queued voice by ID",
        "description": "Retrieves voice generations that are currently queued or have been processed",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the voice generation to fetch",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FetchVoiceRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Fetched voice response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VoiceResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
    "/voice/lyrics_generator": {
      "post": {
        "summary": "Generate lyrics from prompt",
        "description": "Generates lyrics based on a text prompt",
        "requestBody": {
          "required": true,
